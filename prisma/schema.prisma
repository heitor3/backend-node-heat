// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           String @id @default(uuid())
  name         String
  git_id       Int
  login        String
  public_repos Int
  followers    Int
  following    Int
  location     String
  html_url     String
  avatar_url   String

  @@map("users")
}
